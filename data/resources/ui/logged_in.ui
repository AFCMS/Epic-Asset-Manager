<?xml version="1.0" encoding="UTF-8"?>
<interface>
    <object class="GtkSizeGroup">
        <widgets>
            <widget name="home_category"/>
            <widget name="assets_category"/>
            <widget name="plugins_category"/>
            <widget name="games_category"/>
        </widgets>
    </object>
    <requires lib="gtk" version="4.0"/>
    <template class="EpicLoggedInBox" parent="GtkBox">
        <property name="hexpand">1</property>
        <property name="vexpand">1</property>
        <child>
            <object class="GtkBox" id="navigation">
                <property name="orientation">vertical</property>
                <property name="hexpand">false</property>
                <child>
                    <object class="EpicSidebarCategory" id="home_category">
                        <property name="icon-name">go-home</property>
                        <property name="tooltip-text" translatable="yes">Home</property>
                    </object>
                </child>
                <child>
                    <object class="EpicSidebarCategory" id="assets_category">
                        <property name="icon-name">x-package-repository</property>
                        <property name="tooltip-text" translatable="yes">Assets</property>
                    </object>
                </child>
                <child>
                    <object class="EpicSidebarCategory" id="plugins_category">
                        <property name="icon-name">application-x-addon</property>
                        <property name="tooltip-text" translatable="yes">Plugins</property>
                    </object>
                </child>
                <child>
                    <object class="EpicSidebarCategory" id="games_category">
                        <property name="icon-name">input-gaming</property>
                        <property name="tooltip-text" translatable="yes">Games</property>
                    </object>
                </child>
                <child>
                    <object class="GtkBox">
                        <property name="valign">end</property>
                        <property name="halign">fill</property>
                        <property name="hexpand">true</property>
                        <property name="vexpand">1</property>
                        <child>
                            <object class="GtkButton" id="expand_button">
                                <property name="receives-default">1</property>
                                <property name="action-name">loggedin.expand</property>
                                <property name="tooltip-text" translatable="yes">Expand Sidebar</property>
                                <property name="halign">fill</property>
                                <property name="hexpand">true</property>
                                <property name="valign">center</property>
                                <property name="label">></property>
                            </object>
                        </child>
                    </object>
                </child>
            </object>
        </child>
        <child>
            <object class="GtkSeparator">
                <property name="orientation">vertical</property>
            </object>
        </child>
        <child>
            <object class="GtkOverlay" id="MainOverlay">
                <property name="hexpand">1</property>
                <property name="vexpand">1</property>
                <property name="child">
                    <object class="GtkBox">
                        <property name="hexpand">1</property>
                        <property name="vexpand">1</property>
                        <property name="orientation">vertical</property>
                        <child>
                            <object class="GtkSearchEntry" id="search"/>
                        </child>
                        <child>
                            <object class="GtkScrolledWindow">
                                <property name="hexpand">1</property>
                                <property name="vexpand">1</property>
                                <property name="child">
                                    <object class="GtkViewport">
                                        <property name="child">
                                            <object class="GtkFlowBox" id="asset_flow">
                                                <property name="can-focus">0</property>
                                                <property name="halign">start</property>
                                                <property name="valign">start</property>
                                                <property name="homogeneous">1</property>
                                                <property name="max-children-per-line">20</property>
                                            </object>
                                        </property>
                                    </object>
                                </property>
                            </object>
                        </child>
                    </object>
                </property>
                <child type="overlay">
                    <object class="GtkRevealer" id="progress_revealer">
                        <property name="valign">start</property>
                        <property name="child">
                            <object class="GtkProgressBar" id="loading_progress">
                                <property name="can-focus">0</property>
                            </object>
                        </property>
                    </object>
                </child>
            </object>
        </child>
    </template>
</interface>
